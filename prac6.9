#include <iostream>
#include <fstream>
#include <string>

int main() {
   
    std::ifstream inputFile("test.txt");

    if (!inputFile.is_open()) {
        std::cerr << "Failed to open the file!" << std::endl;
        return 1;
    }

    std::string line;
    while (std::getline(inputFile, line)) {
        std::cout << line << std::endl;
    }

    inputFile.close();

    return 0;
}
